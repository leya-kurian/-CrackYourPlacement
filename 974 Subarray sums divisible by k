class Solution(object):
    def subarraysDivByK(self, nums, k):
        ps=0
        d={0:1}
        c=0
        for i in nums:
            ps+=i
            mod=ps%k
            if mod<0:
                mod+=k
            if mod in d:
                c+=d[mod]
                d[mod]+=1
            else:
                d[mod]=1
        return c
