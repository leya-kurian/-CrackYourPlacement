class Solution(object):
    def maxArea(self, height):
        i=0
        j=len(height)-1
        a=0
        b=len(height)-1
        while i<=j:
            if height[i]<height[j]:
                l=height[i]
            else:
                l=height[j]
            if a<l*b:
                a=l*b
            if height[i]<height[j]:
                i+=1
            else:
                j-=1
            b-=1
        return a
            



        
